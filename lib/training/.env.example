# FineTuneLab Training Worker Configuration
# Copy this file to .env and fill in your values

# ===================================================================
# REQUIRED: Next.js API Configuration
# ===================================================================
# URL of your FineTuneLab deployment (where the web UI is hosted)
NEXT_PUBLIC_BASE_URL=https://finetunelab.ai
NEXT_PUBLIC_APP_URL=https://finetunelab.ai

# ===================================================================
# REQUIRED: Supabase Database Configuration
# ===================================================================
# Get these from your FineTuneLab dashboard or Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ===================================================================
# OPTIONAL: Worker Configuration
# ===================================================================
# Unique identifier for this worker (auto-generated if not set)
# WORKER_ID=my-gaming-pc

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# ===================================================================
# OPTIONAL: GPU Configuration
# ===================================================================
# Which GPUs to use (comma-separated, or 'all')
# 0 = first GPU, 1 = second GPU, etc.
CUDA_VISIBLE_DEVICES=0

# ===================================================================
# OPTIONAL: Advanced Settings
# ===================================================================
# Retry interval for failed persistence (seconds)
# RETRY_PERSIST_INTERVAL=300

# Health check interval (seconds)
# HEALTH_CHECK_INTERVAL=30
