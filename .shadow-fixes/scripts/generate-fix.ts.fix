#!/usr/bin/env npx tsx

/**
 * Intelligent Code Fixer (Shadow Fixer)
 *
 * Uses Qwen 2.5 Coder to generate fixes for lint/type errors in the background.
 * Creates a .fix file that can be applied by the user.
 *
 * Usage:
 *   npx tsx scripts/generate-fix.ts <file_path> <error_log>
 */

import { execSync } from 'child_process';
import fs from 'fs';
import path from 'path';

const MODEL = process.env.MODEL || 'qwen2.5-coder:32b';
const filePath = process.argv[2];
const errorLog = process.argv[3];

if (!filePath || !errorLog) {
  console.error('Usage: npx tsx scripts/generate-fix.ts <file_path> <error_log>');
  process.exit(1);
}

if (!fs.existsSync(filePath)) {
  console.error(`File not found: ${filePath}`);
  process.exit(1);
}

async function callOllama(prompt: string): Promise<string> {
  try {
    const response = await fetch('http://localhost:11434/api/generate', {
      method: 'POST',
      headers: { 'Content-Type': 'application/json' },
      body: JSON.stringify({
        model: MODEL,
        prompt,
        stream: false,
        options: { temperature: 0.2, num_predict: 4000 }, // Low temp for precision
      }),
    });

    if (!response.ok) throw new Error(`Ollama error: ${response.statusText}`);
    const data = await response.json();
    return data.response;
  } catch (error) {
    console.error('Failed to call Ollama:', error);
    process.exit(1);
  }
}

async function generateFix() {
  const fileContent = fs.readFileSync(filePath, 'utf-8');
  const fileName = path.basename(filePath);

  const prompt = `You are an expert TypeScript/React developer.
I have a file that is failing linting or type checking.
Please fix the errors described below.

FILE: ${fileName}
ERRORS:
${errorLog}

CURRENT CODE:
\`\`\`typescript
${fileContent}
\`\`\`

INSTRUCTIONS:
1. Return ONLY the complete, fixed file content.
2. Do not include any markdown formatting (no \`\`\`typescript blocks).
3. Do not include any explanations.
4. Maintain the existing coding style.
5. Fix ONLY the reported errors.

FIXED CODE:`;

  console.log(`Generating fix for ${fileName}...`);
  let fixedCode = await callOllama(prompt);

  // Clean up markdown if the model ignored instructions
  fixedCode = fixedCode.trim();
  // Remove opening code block markers (e.g., ```typescript, ```, ```ts)
  fixedCode = fixedCode.replace(/^```[a-zA-Z]*\n?/i, '');
  // Remove closing code block markers
  fixedCode = fixedCode.replace(/```$/i, '');
  fixedCode = fixedCode.trim();

  const projectRoot = process.cwd();
  const absoluteFilePath = path.resolve(filePath);
  const relativePath = path.relative(projectRoot, absoluteFilePath);
  const shadowDir = path.join(projectRoot, '.shadow-fixes');
  const fixPath = path.join(shadowDir, relativePath + '.fix');

  // Ensure directory exists
  fs.mkdirSync(path.dirname(fixPath), { recursive: true });

  fs.writeFileSync(fixPath, fixedCode);
  console.log(`Fix saved to ${fixPath}`);
}

generateFix();
